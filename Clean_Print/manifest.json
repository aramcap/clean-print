{
  "manifest_version": 2,
  "name": "Clean&Print",
  "description": "This extension prepare the webpage to print a clean page, without headers, footers, ...",
  "version": "1.0",
  "icons": { "32": "icon32.png",
    "64": "icon64.png",
   "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}